<h1>Coin MarketCap Api</h1>
<p>Crypro currency application</p>

<br/><br/>


<% @coins %>
<% for x in @coins %>
  <% for coin in @my_coins %>
    <% if coin == x['symbol'] %>
      <%= x['name'] %>: $<%= x['price_usd'] %><br />
      Rank: <%= x['rank'] %><br /></br />
    <% end %>
  <% end %>
<% end %>
